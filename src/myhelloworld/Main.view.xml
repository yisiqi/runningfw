<OpenUI5:View controllerName="inspur.gsp.myhelloworld.Main"
	height="600px" width="800px" xmlns:Controls0="sap.ui.commons"
	xmlns:Layout="sap.ui.commons.layout" xmlns:OpenUI5="sap.ui.core.mvc">
	<Layout:AbsoluteLayout>
		<Layout:PositionContainer left="200px" top="60px">
			<Controls0:Button height="35px" id="btn1" press="onBt1Click" text="{path:'i18n>mybutton.label'}" width="62px"/>
		</Layout:PositionContainer>
		<Layout:PositionContainer left="60px" top="100px">
			<Controls0:TextView id="textView1" text="{oModel>/title}" width="240px"/>
		</Layout:PositionContainer>
		<Layout:PositionContainer left="60px" top="60px">
			<Controls0:TextField id="textField1" placeholder="{path:'i18n>myfield.placeholder'}" width="130px"/>
		</Layout:PositionContainer>
	</Layout:AbsoluteLayout>
</OpenUI5:View>