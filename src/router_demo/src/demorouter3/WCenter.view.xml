<OpenUI5:View 
	controllerName="inspur.gsp.WCenter"
	xmlns="sap.ui.commons"
	xmlns:Layout="sap.ui.commons.layout" 
	xmlns:Formns="sap.ui.layout.form"
	xmlns:OpenUI5="sap.ui.core.mvc"
	xmlns:Core="sap.ui.core"
	xmlns:M="sap.m"
	xmlns:html="http://www.w3.org/1999/xhtml"
	>
	<Layout:VerticalLayout>
		<Label id="wcenterTitle" class="label"/>
		<Label text="任务列表"/>
		<html:ul>
			<html:li>
				<Link text="任务1" press="onSelectTask" />
			</html:li>
			<html:li>
				<Link text="任务2" press="onSelectTask"/>
			</html:li>			
		</html:ul>
	</Layout:VerticalLayout>
	
	
</OpenUI5:View>