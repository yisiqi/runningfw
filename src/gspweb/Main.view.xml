              <TreeNode text="Bean" key="Bean"/>
            </TreeNode>
            <TreeNode text="Fruits" expanded="true" key="Fruits" >
              <TreeNode text="Apple" key="Apple"/>
              <TreeNode text="Orange" key="Orange"/>
              <TreeNode text="Lemon" key="Lemon"/>
